{
  "name": "core",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node node_modules/vite/bin/vite.js dev --host",
    "start": "vite",
    "build": "rm -rf dist && bunx --bun vite build --base=/core/",
    "build-checker": "vue-tsc -b && vite build",
    "lint": "eslint . --fix",
    "preview": "vite preview --host"
  },
  "dependencies": {
    "pinia": "^3.0.4",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@module-federation/vite": "^1.9.2",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "terser": "^5.44.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  }
}
